<div class="reportSection tableContainer">
  {{#hasModified}}
    <h4>Total Cumulative Effects</h4>
    <table>
      <thead>
        <tr>
          <th style="text-align:center;"></th>
          <th>Mean</th>
          <th>Total</th>
          <th>Minimum</th>
          <th>Maximum</th>
        </tr>
      </thead>
      <tbody>
        {{#totals}}
        <tr>
          <td>{{VERSION}}</td>
          <td>{{MEAN}}</td>
          <td>{{TOTAL}}</td>
          <td>{{MIN}}</td>
          <td>{{MAX}}</td>
        </tr>
        {{/totals}}
      </tbody>
    </table>
  </div>
{{/hasModified}}
{{^hasModified}}
    <h4>Total Cumulative Effects</h4>
    <table>
      <thead>
        <tr>
          <th>Mean</th>
          <th>Total</th>
          <th>Minimum</th>
          <th>Maximum</th>
        </tr>
      </thead>
      <tbody>
        {{#totals}}
        <tr>
          <td style="text-align:center;">{{MEAN}}</td>
          <td>{{TOTAL}}</td>
          <td>{{MIN}}</td>
          <td>{{MAX}}</td>
        </tr>
        {{/totals}}
      </tbody>
    </table>
    <tfoot>
      <tr colspan="4">
        <td><div style="width:100%;text-align:center;margin-top:5px;"><i>Note: no activities were changed from the default values</i></div></td>
      </tr> 
    </tfoot>
  </div>
{{/hasModified}}

<div class="reportSection tableContainer">
  <h4>Cumulative Effects For Each Activity</h4>
    <p>
      <span>Show Scores for Habitats:</span>
      <span><select class="chosen-habs">
        {{#habitatsForStressors}}
          <option {{sel}} value="{{VAL}}">{{DISPLAY}}</option>
        {{/habitatsForStressors}}
      </select>
      </span>
    </p>

  {{#d3IsPresent}}
    
      <table class="stressor_table">
        <thead>
          <tr>
            <th style="width:60%;"><a class="stressor_name sort_down" href="#">Activity</a></th>
            <th class="sorting_col"><a class="stressor_perc_adj sort_up" href="#">% Adjustment (100 is unmodified)</a></th> 
            <th><a class="stressor_perc_tot sort_down" href="#">% of Total Impact</a></th>
          </tr>
        </thead>
        <tbody class="stressor_values"></tbody>
      </table>
      <div class='no-stressor-results'>There are no activities with cumulative impacts &gt; 0</div>
      <div class="show-nonzero-checkbox">
      <input class="show_nonzero" checked type="checkbox" name="show_nonzero" value="show_nonzero">
        <span>Only show activities with cumulative impacts &gt; 0</span>
      </div>
  {{/d3IsPresent}}

</div>